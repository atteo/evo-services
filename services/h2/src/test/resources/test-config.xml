<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://moonshine.atteo.org/1.0 config.xsd">
	<btm>
		<transactionTimeout>5</transactionTimeout>
	</btm>
	<transactional-annotation/>
	<h2>
		<!--
			This is the default:
			<url>jdbc:h2:${dataHome}/database;AUTO_SERVER=TRUE</url>

